{
  "name": "click-battle-next",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "node ask-update-backup.js",
    "dev:start": "concurrently -p \"[{name}]\" -n \"NEXT,FIREBASE\" -c \"bgBlack.bold,#FF8800.bold\" \"next dev\" \"yarn emulators\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "emulators": "firebase emulators:start --import=./backups --only database,auth,firestore ",
    "lint:es": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "lint:quiet": "next lint --quiet src",
    "check:tsc": "tsc --noEmit -p . --pretty",
    "check:i18n": "node check-translations.js",
    "export": "yarn build && next export",
    "clear-all": "rm -rf .next _static node_modules",
    "re-start": "rm -rf .next _static node_modules && yarn install && yarn dev",
    "re-build": "rm -rf .next _static node_modules && yarn install && yarn build",
    "update-backup": "npm run export-firestore && npm run copy-bucket-data",
    "export-firestore": "gsutil -m rm -r gs://click-battle-mp.appspot.com/backups && gcloud firestore export gs://click-battle-mp.appspot.com/backups",
    "copy-bucket-data": "gsutil -m cp -r gs://click-battle-mp.appspot.com/backups ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.5.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^2.1.8",
    "@leandrolescano/click-battle-core": "^1.2.1",
    "@next/bundle-analyzer": "^13.0.2",
    "@sentry/nextjs": "^10.26.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "accept-language": "^3.0.18",
    "adblock-detector": "^2.1.0",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.2",
    "chroma-js": "^3.1.1",
    "clsx": "^2.1.1",
    "firebase": "^9.5.0",
    "firebase-admin": "^12.0.0",
    "framer-motion": "^7.2.0",
    "fs": "^0.0.1-security",
    "i18next": "^23.11.4",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-resources-to-backend": "^1.2.1",
    "lottie-web": "^5.8.1",
    "moment": "^2.30.1",
    "next": "^14.2.5",
    "nodemailer": "^6.9.12",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-flip-move": "^3.0.4",
    "react-flip-toolkit": "^7.2.4",
    "react-i18next": "^14.1.1",
    "react-rating-stars-component": "^2.2.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "sweetalert2": "^11.10.6",
    "sweetalert2-react-content": "^5.0.3",
    "tailwind-merge": "^2.5.2",
    "web-vitals": "^2.1.2",
    "webpack": "^5.64.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.1",
    "@types/chroma-js": "^2.4.4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.13",
    "@types/react": "^18.3.4",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.47",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.9.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
